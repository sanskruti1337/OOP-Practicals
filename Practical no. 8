#include <iostream>
#include <vector>
using namespace std;

// Template for Bubble Sort
template <typename T>
void bubbleSort(vector<T>& arr) {
    for (int i = 0; i < arr.size() - 1; i++) {
        for (int j = 0; j < arr.size() - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                T temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
}

// Template for Display
template <typename T>
void displayArray(vector<T> arr) {
    for (T x : arr)
        cout << x << " ";
    cout << endl;
}

int main() {
    vector<int> intArr = {5, 2, 9, 1, 6};
    vector<float> floatArr = {3.5, 1.2, 4.8, 2.1};

    cout << "Before sorting (int): ";
    displayArray(intArr);
    bubbleSort(intArr);
    cout << "After sorting (int): ";
    displayArray(intArr);

    cout << "\nBefore sorting (float): ";
    displayArray(floatArr);
    bubbleSort(floatArr);
    cout << "After sorting (float): ";
    displayArray(floatArr);

    return 0;
}

